<div class="container" id="admin_enduser_index">
  <h5>会員一覧</h5>
  <table class="end_user_table">
    <thead>
      <tr class="end_user_table_head">
        <th scope="col-md-1">会員id</th>
        <th scope="col-md-4">氏名</th>
        <th scope="col-md-5">メールアドレス</th>
        <th scope="col-md-2">ステータス</th>
      </tr>
    </thead>
    <tbody>
      <% @user.each do |user| %>
        <tr class="end_user_table_body">
          <td scope="row"><%= user.id %></td>
          <td><%= user.last_name %><%= user.first_name %></td>
          <td><%= user.email %></td>
          <td>
            <%if user.is_unsubscribed == true %>
              <a class="valid">有効</a>
            <% else %>
              <a class="invalid">退会</a>
            <% end %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
